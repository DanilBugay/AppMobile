<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.examples.testproject.login">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainLogin"></activity>


        <activity
            android:name="com.examples.testproject.file.MainActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.examples.testproject.file.UploadingActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.examples.testproject.file.BrowseActivity" />


        <activity android:name=".HelloActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RestorePasswordActivity"></activity>

        <activity android:name=".RegisterActivity"></activity>


        <activity android:name=".LoginResult"></activity>
    </application>

</manifest>
